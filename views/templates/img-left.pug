featleft
  mixin featleft(title, subtitle, picture, body, bodyb, bodyc, icon, call)
      .featlbg
        .featbg  
          .featleftgrid
            .projpic
              if block
                block
            .featparhold
              .featnowsub
                .featpar
                  h3.title=title
                  h4.subtitle=subtitle
                  p.projwords=body
                  p.projwords=bodyb
                  p.projwords=bodyc
                  //
                    .littlspace
                    .buttonhold
                      .texthold
                        .iconhold
                          img.buttonicon(src=icon)
                        p.buttontext
                          a.buttonlink(href="https://docs.google.com/document/d/1xvhU7kHlzOAnvrB9Dy0Bvjg28JqEhpTtez6MRo-z7sM/edit")=call
